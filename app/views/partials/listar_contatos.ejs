<table>
    <%
        for ( var cont = 0; cont < dadosBlog.length; cont++ ) {
            %>
            <tr><td>-------------------Registro </td><td>-<%= cont+1 %>----------------</td></tr>
            <tr>
                <td>ID[<%= cont+1 %>] = </td> <td> <%= dadosBlog[cont].id %></td>
            </tr>
            <tr>
                <td>Nome[<%= cont+1 %>] = </td> <td> <%= dadosBlog[cont].name %></td>
            </tr>
            <tr>
                <td>Email[<%= cont+1 %>] = </td> <td> <%= dadosBlog[cont].email %></td>
            </tr>
            <tr>
                <td>Telefone[<%= cont+1 %>] = </td> <td> <%= dadosBlog[cont].phone %></td>
            </tr>
            <tr><td><a href="/delete/<%= dadosBlog[cont].id %>">EXCLUIR</a></td><td>--------------------</td></tr>
            <%
        }
    %>
</table>